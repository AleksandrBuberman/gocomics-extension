{
  "manifest_version": 3,
  "name": "GoComics image link",
  "version": "1.0",
  "description": "Provide link for the GoComics comic image",
  "icons": {
    "16": "i16.png"
  },
  "content_scripts": [
    {
      "js": ["gocomics.js"],
      "matches": [
        "https://www.gocomics.com/*"
      ]
    }
  ]
}